#include <stdio.h>
#include <stdlib.h>

// this will hollow square pattern
int main()
{ // for row 
    for (int a = 1; a <= 5; a++)
    { // for column
        for (int b = 1; b <= 5; b++)
        {
            if (a == 1 || a == 5 || b == 1 || b == 5 || (a == 2 && b == 2) || (a == 2 && b == 4) || (a == 3 && b == 3) || (a == 4 && b == 2)||(a==4 && b == 4)) // this will only make star 
            {                                                                                                                                                       
                printf("*");
            }
            else
            {
                printf(" ");
            }
        }
        printf("\n");
    }
}
